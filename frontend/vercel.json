{
  "version": 2,
  "name": "pilahpintar-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL_PRODUCTION": "https://pilahpintar-backend.vercel.app/api",
    "REACT_APP_NAME": "PilahPintar",
    "REACT_APP_VERSION": "1.0.0",
    "GENERATE_SOURCEMAP": "false"
  }
}
