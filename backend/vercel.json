{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.js"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri"
  },
  "functions": {
    "src/app.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}
